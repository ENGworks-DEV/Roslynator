<?xml version="1.0" encoding="utf-8"?>
<Analyzers>
  <Analyzer Identifier="FixFormattingOfMethodChain">
    <Id>RCS0054</Id>
    <Title>Fix formatting of a method chain.</Title>
    <Category>Formatting</Category>
    <DefaultSeverity>Info</DefaultSeverity>
    <IsEnabledByDefault>false</IsEnabledByDefault>
    <Summary>This analyzer:
* fixes indentation of multiline method chain
* formats each method call on a separate line
</Summary>
    <Samples>
      <Sample>
        <Before><![CDATA[]]></Before>
        <After><![CDATA[]]></After>
      </Sample>
    </Samples>
  </Analyzer>
</Analyzers>